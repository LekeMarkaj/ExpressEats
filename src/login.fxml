<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="402.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
   <children>
      <ImageView fitHeight="74.0" fitWidth="95.0" layoutX="150.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@delivery.png" />
         </image>
      </ImageView>
      <Label layoutX="28.0" layoutY="201.0" />
      <TextField fx:id="emailText" alignment="CENTER" layoutX="115.0" layoutY="145.0" promptText="Email" />
      <Button fx:id="loginButton" layoutX="164.0" layoutY="284.0" mnemonicParsing="false" onAction="#testLoginButton" text="Log In" textFill="#800ee4" />
      <PasswordField fx:id="passwordText" alignment="CENTER" layoutX="115.0" layoutY="197.0" promptText="Password" />
      <Label fx:id="emailError" alignment="CENTER" layoutX="110.0" layoutY="171.0" prefHeight="18.0" prefWidth="152.0" textFill="RED" />
      <Label fx:id="passError" alignment="CENTER" layoutX="111.0" layoutY="224.0" prefHeight="18.0" prefWidth="159.0" textAlignment="RIGHT" textFill="RED" />
      <Button fx:id="backButton" alignment="CENTER" layoutX="175.0" layoutY="344.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#goBack" prefHeight="20.0" prefWidth="30.0" text="&lt;" textAlignment="CENTER" textFill="#800ee4" />
   </children>
</AnchorPane>
